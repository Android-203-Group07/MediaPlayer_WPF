<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:DemoUI.ViewModel"
                    xmlns:view="clr-namespace:DemoUI.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MusicLibraryVM}">
        <view:MusicLibrary />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:VideoLibraryVM}">
        <view:VideoLibrary />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:PlaylistVM}">
        <view:Playlist />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UserControlVM}">
        <view:UserControl1 />
    </DataTemplate>
</ResourceDictionary>